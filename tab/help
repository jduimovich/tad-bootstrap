SCRIPTDIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" >/dev/null 2>&1 && pwd )" 
PIPELINES_DIR=$(realpath $SCRIPTDIR/../pipelines)  

echo "Commands available"
ls -a -I . -I .. -I tab $SCRIPTDIR 
echo
echo "Secure Build Pipelines available from $PIPELINES_DIR"
ls -a -I . -I .. -I repository.yaml $PIPELINES_DIR  



